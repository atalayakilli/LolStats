        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6721141646053572~9859355252" />

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SummonerInfos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ChampionMasteries"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ChampionRotations"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LastPlayed"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LiveMatch"
            android:screenOrientation="portrait" />
        <activity android:name=".LiveMatchInfo"></activity>
    </application>

</manifest>